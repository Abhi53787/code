def bubb(self):
    n=len(arr)
    for i in range(n-1):
        for j in range(0,n-i-1):
            if(arr[j]>arr[j+1]):
                temp=arr[j]
                arr[j]=arr[j+1]
                arr[j+1]=temp
    
    
    
    
    
arr=[7,8,6,2,1,4]
bubb(arr)
for i in range(len(arr)):
     print(arr[i], end=" ")
