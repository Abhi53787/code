nterms = int(input("How many terms? "))

n1, n2 = 0, 1
count = 0


if nterms <= 0:
   print("Please enter only positive integer ğŸ™ƒ")

elif nterms == 1:
   print("Fibonacci sequence of given number is ":",n1)

else:
   print("Fibonacci sequence of given number is ğŸ˜Œ:")
   while count < nterms:
       print(n1)
       nth = n1 + n2
       n1 = n2
       n2 = nth
       count += 1
