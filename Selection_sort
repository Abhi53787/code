def Selection_sort(self):
    n=len(arr)
    for i in range(n-1):
        small=i
        for j in range(i+1,n):
            if(arr[j]<arr[small]):
                small=j
        temp=arr[small]
        arr[small]=arr[i]
        arr[i]=temp
    
    
    
    
    
arr=[7,8,6,2,1,4]
Selection_sort(arr)
for i in range(len(arr)):
     print(arr[i], end=" ")
